{
  "name": "mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start:dev": "APP_VARIANT=development npx expo start --dev-client",
    "build_ios:dev": "APP_VARIANT=development npx expo run:ios --device",
    "build_ios:staging": "APP_VARIANT=staging npx eas build --platform ios --profile staging --auto-submit-with-profile=staging",
    "build_ios:production": "APP_VARIANT=staging npx eas build --platform ios --profile production",
    "build_android:dev": "APP_VARIANT=dev npx expo run:android --device",
    "release_android:staging": "APP_VARIANT=staging npx expo run:ios --device",
    "release_android:production": "APP_VARIANT=production npx expo run:ios --device",
    "prebuild": "npx expo prebuild",
    "easBuildStaging:ios": "eas build --platform ios --profile staging",
    "easSubmitStaging:ios": "eas submit -p ios --profile staging --latest",
    "easBuildProduction:ios": "eas build --platform ios",
    "easSubmitProduction:ios": "eas submit -p ios --profile production --latest",
    "ios:device_development": "npx react-native run-ios --device yphone --scheme 'Mekka_development'",
    "ios:simulator_development": "npx react-native run-ios --scheme 'Mekka_development'"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "7.6.1",
    "@react-native-community/slider": "4.4.3",
    "@react-native-picker/picker": "2.5.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@rnmapbox/maps": "^10.1.28",
    "@shopify/flash-list": "1.4.3",
    "@tanstack/react-query": "^5.52.2",
    "@types/react": "~18.2.34",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "expo": "^49.0.16",
    "expo-av": "~13.6.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.11",
    "expo-haptics": "~12.6.0",
    "expo-image": "~1.3.4",
    "expo-image-picker": "~14.5.0",
    "expo-notifications": "~0.20.1",
